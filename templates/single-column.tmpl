%%%%%%  CV STARTS HERE  %%%%%%%%%%%%%%%%%%%%%%%%%%%%


\begin{document}

%----------HEADING-----------------
\begin{center}
{\headingfont\textcolor{accentColor}{\textbf{\Huge \scshape {{.Name}}}}} \\ \vspace{1pt}
\small \href{tel:{{.Contact.Phone}}}{ {{- .Contact.Phone}}} $|$ \href{mailto:{{.Contact.Email}}}{ {{- .Contact.Email}}} $|$
\href{https://www.{{.Contact.LinkedIn}}}{ {{- .Contact.LinkedIn}}} $|$
\href{https://www.{{.Contact.GitHub}}}{ {{- .Contact.GitHub}}}
\end{center}

%-----------OVERVIEW-----------------
\section{Overview}
\small {{latexEscape .Summary}}

%-----------EXPERIENCE-----------------
\section{Experience}
\resumeSubHeadingListStart
{{ range $experience :=.Experience -}}
    {{ range $index, $position := $experience.Positions -}}
        {{ if eq $index 0 -}}
            \resumeSubheading
                { {{- $experience.Company}}}%{ {{- $experience.Location}}}
                { {{- $position.Title}}{{if ne $position.Type ""}} ({{$position.Type}}){{end}}}{ {{- $position.StartDate}} — {{- $position.EndDate}}}
            \resumeItemListStart
            {{- range $position.Responsibilities }}
                \resumeItem{ {{- latexEscape .}} }
            {{- end }}
            \resumeItemListEnd
        {{ else -}}
            \resumeSubSubheading
            { {{- $position.Title}}}{ {{- $position.StartDate}} — {{- $position.EndDate}}}
            \resumeItemListStart
            {{- range $position.Responsibilities }}
                \resumeItem{ {{- latexEscape .}} }
            {{- end }}
            \resumeItemListEnd
        {{ end -}}
    {{ end -}}
{{ end -}}
\resumeSubHeadingListEnd

%-----------PROJECTS-----------
\section{Projects}
\resumeSubHeadingListStart
{{ range .Projects -}}
    \resumeProjectHeading
    {\textbf{ {{- .Name}}} $|$ \emph{ {{- join .Technology ", "}}}}{ \href{ {{- .Github}}}{\underline{Github}}}
    %\small{ { {{- .Description}} \vspace{2pt}}}
    \resumeItemListStart
        {{- range .Items }}
            \resumeItem{ {{- latexEscape .}} }
        {{- end }}
    \resumeItemListEnd
{{ end -}}
\resumeSubHeadingListEnd

%--------SKILLS------------
\section{Technical Skills}
\begin{itemize}[leftmargin=0.15in, label={}]
\small{\item{
{{- range $cat := (orderedSkills .Skills) }}
    \textbf{ {{- $cat.Name}}}{: {{ range $i, $v := $cat.Values }}{{if $i}}, {{end}}{{$v}}{{end}}} \\
{{- end }}
}}
\end{itemize}

%-----------EDUCATION-----------------
\section{Education}
\resumeSubHeadingListStart
{{- range .Education}}
    \resumeSubheadingLocation
    { {{- .Institution}}{{if ne .College ""}}, {{.College}}{{end}}}{ {{- .StartDate}} — {{.EndDate}}}
    { {{- .Degree}}{{if ne .GPA ""}}; GPA: {{.GPA}}{{end}}}
{{ end -}}
\resumeSubHeadingListEnd

%-------------------------------------------
\end{document}
